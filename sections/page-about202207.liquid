<script type="text/javascript">
  window.addEventListener("DOMContentLoaded", function () {
    $("#PageContainer").addClass("page-about202207");
    var md = new MobileDetect(window.navigator.userAgent);
     if(!md.mobile()){
        var justAboutHeight = function(){
           var selections = $("#PageContainer .shopify-section");
           selections.each(function(i){
              if(i===3 || i===5){
                if($(this).find(".custom__item").length===2){
                  var left = $(this).find(".custom__item").eq(0);
                  var right = $(this).find(".custom__item").eq(1); 
                  var leftH = left[0].getBoundingClientRect().height -4; /** jquery的height失去了精度 **/
                  right.height(leftH);
                }
              }else if(i===4){
                var left = $(this).find(".custom__item").eq(0);
                var right = $(this).find(".custom__item").eq(1); 
                var rightH = right[0].getBoundingClientRect().height -4; /** jquery的height失去了精度 **/
                left.height(rightH);
              }
           });
       };
       justAboutHeight();
       $(window).resize(function(){
          justAboutHeight();
       });
     }
  });;
</script>