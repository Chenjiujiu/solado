<div id="tracktor" class="themeLight __web-inspector-hide-shortcut__" data-theme="Light" data-progress-bar-color="#E1B9EB">
<form id="tracktorContainer" >
  <div id="tracktorOrderForm">
      <h1><span>Track Your Order</span></h1>
      <div class="field"><label for="tracktorOrderName"><span>Order Number</span></label><input required="" aria-required="true" id="tracktorOrderName" type="text" name="order"></div>
      <div class="field"><label for="tracktorOrderEmail"><span>Email</span></label><input required="" aria-required="true" id="tracktorOrderEmail" type="text" name="userEmail"></div>
      <br style="clear:both">
      <div class="captcha-wrap" style="display: none;justify-content: center;"></div>
      <button id="tracktorTrack" class="button-enter btn styled-submit button"><span>Track</span></button>
  </div>
  <section id="tracktorResponseContainer"></section>
</form>

</div>
<script type="text/javascript">
  function spOnLoadCallback() {}
        window.addEventListener('DOMContentLoaded',function(){
           $("#tracktorContainer").submit(function(event){
            event.preventDefault();
         });
        var verifyNumberOrEmail = function(){ 
              var order = $("#tracktorOrderName").val().trim(); 
              var email = $("#tracktorOrderEmail").val().trim(); 
              if(order!=='' && email!==''){
                  /**console.log('提交一次');**/
                  var domain = "https://www.solado.com";
                  if(order.indexOf("SOLADOEU") > -1){
                    domain = "https://eu.solado.com";
                  }else if(order.indexOf("SOLADOUK") > -1){
                    domain = "https://uk.solado.com";
                  }
                  var orderEncode = encodeURIComponent(order);
                  var emailEncode = encodeURIComponent(email.replace('@','_-_').split('').reverse().join(''));
                  var jumpUrl = domain+"/apps/tracktor/track?order="+orderEncode+"&token="+emailEncode;
                  /**console.log('jumpUrl',jumpUrl);**/
                  location.href =jumpUrl;
              }
          }  
          var $btn =  $("#tracktorTrack");
          $btn.on("click",function(){
            verifyNumberOrEmail();
          });
      });
    </script>
