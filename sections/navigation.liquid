            window.addEventListener('DOMContentLoaded', (event) => {
                /** 只有当屏幕宽度小于1024的时候才会展示在菜单中**/
                if ($(window).width() < 1024) {
                    cloneSiteAndNav();
                }
            });

            function addOtherMobileNavLink(sideNav) {
                var $footer = $("#shopify-section-footer .site-footer").clone(true);
                $footer.find(".newsletter").remove();
                $footer.find(".footer-bottom").remove();
                $footer.find(".socials").remove();
                $footer.find(".paymentCall").remove();
                var $footerBottom = $("#shopify-section-footer .site-footer .footer-bottom").clone(true);
                $(sideNav).append("<div class='clone-footer-wrap'></div>").find(".clone-footer-wrap").append($footer).append($footerBottom);
                if ($(".clone-footer-wrap").height() > ($(".clone-footer-wrap .site-footer").height() + $(".clone-footer-wrap .footer-bottom").height())) {
                    $(".clone-footer-wrap .footer-bottom").addClass("footer-fixed-bottom")
                }

            };

            function cloneSiteAndNav() {
                const changeSiteBox = document.querySelector('#changeSiteBox');
                const sideNav = document.querySelector('#MobileNav');
                if (sideNav) {
                    const li = document.createElement('li');
                    li.appendChild(changeSiteBox);
                    li.classList.add('change-site-box-list-item');
                    sideNav.appendChild(li);
                    changeSiteBox.style.display = 'block';
                    addOtherMobileNavLink(sideNav);
                }
            };
