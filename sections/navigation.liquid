<style>
    .sidebar .sidebar_widget, .mobile-nav {
        -webkit-tap-highlight-color: transparent;
    }

    @media only screen and (min-width: 1025px) {
        #siteNav div.megamenu li.lvl-1 {
            margin-bottom: 0;
            font-weight: 600;
            font-size: 14px;
            line-height: 30px;
        }

        #siteNav div.megamenu li.lvl-1 a.lvl-1 {
            padding: 0;
        }

        #siteNav div.megamenu li.lvl-1 li a {
            padding: 0;
        }
    }

    .nav-custom-image-wrap {
        display: flex;
        margin-top: 7px;
    }

    .nav-custom-image-wrap li:not(:first-child) {
        margin-left: 20px;
    }

    #siteNav .onlyHaveTwo, #siteNav .allHaveThree, #siteNav .bothHaveTwoAndThree {
        margin-right: 35px;
    }

    #siteNav .allHaveThree {
        display: flex;
    }

    #siteNav .bothHaveTwoAndThree {
        display: flex;
    }

    #siteNav .bothHaveTwoAndThree > ul:not(:first-child) {
        margin-left: 40px;
    }

    #siteNav .allHaveThree > ul:not(:first-child) {
        margin-left: 40px;
    }

    @media only screen and (min-width: 1025px) {
        #siteNav div.megamenu {
            padding: 23px 0 36px;
            display: flex;
            justify-content: center;
        }
    }

    @media only screen and (max-width: 1024px) {
        #MobileNav .footer-bottom li a .ad {
            width: auto;
            height: auto;
            position: inherit;
            font-size: 20px;
            min-width: 20px;
            padding: 0;
        }

        #MobileNav .footer-bottom li a {
            padding: 0;
        }

        #MobileNav .clone-footer-wrap {
            flex: auto;
            position: relative;
        }

        #MobileNav .clone-footer-wrap .payment-icon {
            height: 23px;
        }

        #MobileNav .clone-footer-wrap .footer-bottom {
            background: transparent;
        }

        #MobileNav .clone-footer-wrap .footer-bottom.footer-fixed-bottom {
            position: absolute;
            width: 100%;
            bottom: 0;
            left: 0;
        }

        #MobileNav .clone-footer-wrap .footer-bottom .flex > div {
            font-size: 12px;
        }
    }
</style>
{%- if section.blocks.size > 0 -%}
	<ul id="siteNav"
	    class="siteNavigation text-{{ section.settings.menuAlign }}{% if settings.nav_below_logo %}{% unless settings.main_header_fullwidth %} page-width{% endunless %}{% endif %}"
	    role="navigation" data-section-id="site-{{ section.id }}" data-section-type="site-navigation">
		{% for block in section.blocks %}
			{%- assign label = block.settings.title | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
			{%- capture linkLevel1 -%}<a
				href="{% if block.settings.url != blank %}{{ block.settings.url }}{% else %}#{% endif %}" {% if block.settings.newtab %} target="_blank"{% endif %}>{{ block.settings.title | split: '[label' | first }}{% if section.settings.menuarrow %}
				<i class="ad ad-angle-down-l"></i>{% endif %}{% if block.settings.title contains '[label' %}<span
				class="lbl {{ label }}">{{ section.settings[label] }}</span>{% endif %}</a>{%- endcapture -%}
			{%- case block.type -%}
				{%- when 'homelink' -%}
					<li class="lvl1 {% render 'navigation-mark', label: label %}"><a
							href="{{ routes.root_url }}">{{ block.settings.title }}</a></li>

				{%- when 'loginlink' -%}
					{% if customer %}
						<li class="lvl1 {% render 'navigation-mark', label: label %}"><a
								href="{{ routes.account_url }}">{{ 'customer.account.title' | t }}</a></li>
						<li class="lvl1 {% render 'navigation-mark', label: label %}"><a
								href="{{ routes.account_logout_url }}">{{ 'customer.account.logout' | t }}</a></li>
					{% else %}
						<li class="lvl1 {% render 'navigation-mark', label: label %}"><a
								href="{{ routes.account_login_url }}">{{ 'customer.login.form_title' | t }}</a></li>
					{% endif %}

				{%- when 'single_link' -%}
					<li class="lvl1 {% render 'navigation-mark', label: label %}" {{ block.shopify_attributes }}>{{ linkLevel1 | replace: '<i class="ad ad-angle-down-l"></i>', '' }}</li>

				{%- when 'megamenu1' -%}
					{%- if block.settings.title != blank -%}
						<li
						class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %} {% render 'navigation-mark', label: label %}" {{ block.shopify_attributes }}>{{ linkLevel1 }}
					<div
						class="megamenu style1{% if block.settings.imgcall == "right" %} flex{% endif %}{% if block.settings.custom_size %} {{ block.settings.mm_align }}{% else %} mmfull{% endif %}"
						{% if block.settings.custom_size %}style="width:{{ block.settings.mm_size }}px;"{% endif %}>
						{% if block.settings.box_sized %}
						<div class="mm_layout{% if block.settings.imgcall == "right" %} flex{% endif %}">{% endif %}

							{%- capture imgColumns -%}
							{%- capture my_variable -%}{{ block.settings.megamenu1_image }} {{ block.settings.megamenu2_image }}{%- endcapture -%}
							{%- assign products_array = my_variable | split: ' ' -%}
							{%- if products_array.size > 0 -%}
							{%- if block.settings.megamenu1_image != blank -%}
						<li class="grid__item {{ imgcolumns }} imageCol text-center">
							<a href="{{ block.settings.imageurl }}">
								<img class="autoHt lazyload" src="{{ 'spacer.png' | asset_img_url: 'master' }}"
								     data-src="{{ block.settings.megamenu1_image | img_url: 'master' }}" width="228"
								     height="280" alt="{{ block.settings.megamenu1_image.alt }}">
								{% if block.settings.imageTtl1 != blank %}<span
									class="ttl">{{ block.settings.imageTtl1 }}</span>{% endif %}
							</a>
						</li>
					{%- endif -%}
					{%- if block.settings.megamenu2_image != blank -%}
						<li class="grid__item {{ imgcolumns }} imageCol text-center">
							<a href="{{ block.settings.Image2url }}">
								<img class="autoHt lazyload" src="{{ 'spacer.png' | asset_img_url: 'master' }}"
								     data-src="{{ block.settings.megamenu2_image | img_url: 'master' }}" width="228"
								     height="280" alt="{{ block.settings.megamenu2_image.alt }}">
								{% if block.settings.imageTtl2 != blank %}<span
									class="ttl">{{ block.settings.imageTtl2 }}</span>{% endif %}
							</a>
						</li>
					{%- endif -%}
					{%- endif -%}
					{%- endcapture -%}

						{%- capture oneImgColumns -%}
							{%- capture my_variable -%}{{ block.settings.megamenu1_image }} {%- endcapture -%}
							{%- assign products_array = my_variable | split: ' ' -%}
							{%- if products_array.size > 0 -%}
								{%- if block.settings.megamenu1_image != blank -%}
									<li class="grid__item {{ imgcolumns }} imageCol text-center">
										<a href="{{ block.settings.imageurl }}">
											<img class="autoHt lazyload"
											     src="{{ 'spacer.png' | asset_img_url: 'master' }}"
											     data-src="{{ block.settings.megamenu1_image | img_url: 'master' }}"
											     width="228" height="280"
											     alt="{{ block.settings.megamenu1_image.alt }}">
											{% if block.settings.imageTtl1 != blank %}<span
												class="ttl">{{ block.settings.imageTtl1 }}</span>{% endif %}
										</a>
									</li>
								{%- endif -%}
							{%- endif -%}
						{%- endcapture -%}

						{% comment %}导航区域{% endcomment %}
						{% assign havelevelThreeLen = 0 %}
						{% assign levelTwoLen = linklists[block.settings.mm_linklist].links | size %}
						{% comment %}<div>二级导航总个数：{{levelTwoLen}}</div>{% endcomment %}
						{% for link in linklists[block.settings.mm_linklist].links %}
							{%- if link.links != blank -%}
								{% assign havelevelThreeLen = havelevelThreeLen | plus: 1 %}
							{%- endif -%}
						{% endfor %}
						{% comment %}<div>有三级子导航的个数：{{havelevelThreeLen}}</div>{% endcomment %}
						{% assign onlylevelTwoLen = levelTwoLen | minus: havelevelThreeLen %}
						{% comment %}<div>没有三级子导航的个数：{{onlylevelTwoLen}}</div>{% endcomment %}
						{% assign caseStr = "bothHaveTwoAndThree" %}
						{% if levelTwoLen == onlylevelTwoLen %}
							{% assign caseStr = "onlyHaveTwo" %}
						{% endif %}
						{% if levelTwoLen == havelevelThreeLen %}
							{% assign caseStr = "allHaveThree" %}
						{% endif %}
						{% comment %}<span>{{caseStr}}</span>{% endcomment %}

						{% if block.settings.megamenu1_image != blank or block.settings.megamenu2_image != blank %}
							{% include 'mm-links-pc', caseStr %}
							{% comment %}{% for link in linklists[block.settings.mm_linklist].links %}{% include 'mm-links-pc',caseStr %}{% endfor %}{% endcomment %}
							{% comment %}图片区域{% endcomment %}
							<ul class="nav-custom-image-wrap">
								{% if caseStr == "onlyHaveTwo" %}
									{{ imgColumns }}
								{% else %}
									{% if levelTwoLen > 4 %}
										{{ oneImgColumns }}
									{% else %}
										{{ imgColumns }}
									{% endif %}
								{% endif %}
							</ul>
						{%- else -%}
							{% include 'mm-links-pc', caseStr %}
							{% comment %}<ul class="grid flex-item mmWrapper">{% for link in linklists[block.settings.mm_linklist].links %}{% include 'mm-links' %}{% endfor %}</ul>{% endcomment %}
						{%- endif -%}
							{% if block.settings.box_sized %}</div>{% endif %}
						</div>
						</li>
					{%- endif -%}

				{%- when 'megamenu2' -%}
					{%- if block.settings.title != blank -%}
						<li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %} {% render 'navigation-mark', label: label %}" {{ block.shopify_attributes }}>{{ linkLevel1 }}
							<div
								class="megamenu grid--table style2{% if block.settings.custom_size %} {{ block.settings.mm_align }}{% else %} mmfull{% endif %}"
								{% if block.settings.custom_size %}style="width:{{ block.settings.mm_size }}px;"{% endif %}>
							{% if block.settings.box_sized %}
								<div class=" grid--table mm_layout">{% endif %}
									<div class="grid__item prColumn">
										<ul class="grid grid--uniform">
											{%- case block.settings.mmcolumns -%}
												{% when 1 %}{%- assign column = 'large-up--one-whole' -%}
												{% when 2 %}{%- assign column = 'large-up--one-half' -%}
												{% when 3 %}{%- assign column = 'large-up--one-third' -%}
												{% when 4 %}{%- assign column = 'large-up--one-quarter' -%}
												{% when 5 %}{%- assign column = 'large-up--one-fifth' -%}
												{% when 6 %}{%- assign column = 'large-up--one-sixth' -%}
											{%- endcase -%}
											{%- assign collection = collections[block.settings.collection] -%}
											{% for product in collection.products limit: block.settings.pr_limit %}
												<li class="grid__item {{ column }} productCol">{% include 'mm-product' %}</li>
											{% endfor %}
										</ul>
										{%- if block.settings.prodall != blank -%}<a href="{{ collection.url }}"
										                                             class="btn">{{ block.settings.prodall }}</a>{%- endif -%}
									</div>
									{%- if block.settings.ftcl -%}
									<div class="grid__item mmftcolumn text-center"
									     style="background-color:{{ block.settings.ftcl_bg }};width:{{ block.settings.ftcl_width }}%;">
									{%- if block.settings.ft_link != blank -%}<a
											href="{{ block.settings.ft_link }}">{%- endif -%}
											{%- if block.settings.ftcl_image != blank -%}
												{%- assign mm2img_url = block.settings.ftcl_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
												<p><img class="imgFl lazyload"
												        src="{{ block.settings.ftcl_image | img_url: '50x' }}"
												        width="{{ block.settings.ftcl_image.width }}"
												        height="{{ block.settings.ftcl_image.height }}"
												        data-src="{{ mm2img_url }}"
												        data-widths="[180, 360, 540, 720, 900]"
												        data-aspectratio="{{ block.settings.ftcl_image.aspect_ratio }}"
												        data-sizes="auto"
												        alt="{{ block.settings.ftcl_image.alt | escape }}"></p>
											{%- endif -%}
											{%- if block.settings.ftcl_title != blank -%}<h4
												style="color:{{ block.settings.ftcl_txt }};font-size:{{ block.settings.ftcl_title_size }}px;">{{ block.settings.ftcl_title }}</h4>{%- endif -%}
											{%- if block.settings.ftcl_title != blank -%}<p
												style="color:{{ block.settings.ftcl_txt }};font-size:{{ block.settings.ftcl_subtitle_size }}px;">{{ block.settings.ftcl_subtitle }}</p>{%- endif -%}
											{%- if block.settings.ftcl_title != blank -%}<span class="btn"
											                                                   style="color:{{ block.settings.ftcl_txt }};background-color:{{ block.settings.ftcl_bg }}; border:2px solid {{ block.settings.ftcl_txt }};">{{ block.settings.ftcl_btn }}</span>{%- endif -%}
											{%- if block.settings.ft_link != blank -%}</a>{%- endif -%}
									</div>
									{% if block.settings.box_sized %}</div>{% endif %}
							</div>
							{%- endif %}
						</li>
					{%- endif -%}

				{%- when 'megamenu3' -%}
					{%- if block.settings.title != blank -%}
						<li class="lvl1 parent megamenu{% if block.settings.custom_size %} ctsize{% endif %} {% render 'navigation-mark', label: label %}" {{ block.shopify_attributes }}>{{ linkLevel1 }}
							<div
								class="megamenu style3{% if block.settings.custom_size %} {{ block.settings.mm_align }}{% else %} mmfull{% endif %}"
								{% if block.settings.custom_size %}style="width:{{ block.settings.mm_size }}px;"{% endif %}>
							{% if block.settings.box_sized %}
								<div class="mm_layout">{% endif %}
									<ul class="grid mmWrapper">
										{%- capture menucolumns -%}{{ block.settings.ttl1 | remove: " " }} {{ block.settings.ttl2 | remove: " " }} {{ block.settings.ttl3 | remove: " " }} {{ block.settings.ttl4 | remove: " " }} {{ block.settings.ttl5 | remove: " " }}{%- endcapture -%}
										{%- assign columnitems = menucolumns | split: ' ' -%}
										{%- assign mmcolumns = 'large-up--one-fifth' -%}
										{%- assign mm3img = '380x' -%}
										{%- if columnitems.size == 4 -%}
											{% assign mmcolumns = 'large-up--one-quarter' %}
											{% assign mm3img = '475x' %}
										{% elsif columnitems.size == 3 %}
											{% assign mmcolumns = 'large-up--one-third' %}
											{% assign mm3img = '640x' %}
										{% elsif columnitems.size == 2 %}
											{% assign mmcolumns = 'large-up--one-half' %}
											{% assign mm3img = '900x' %}
										{% elsif columnitems.size == 1 %}
											{% assign mmcolumns = 'large-up--one-whole' %}
											{% assign mm3img = '1900x' %}
										{%- endif -%}

										{%- if block.settings.ttl1 != blank -%}
											<li class="grid__item {{ mmcolumns }} lvl-1">
												{%- assign m3label1 = block.settings.ttl1 | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
												<a href="{% if block.settings.ttlurl1 != blank %}{{ block.settings.ttlurl1 }}{% else %}#{% endif %}"
												   class="lvl-1">
													{%- assign img_url1 = block.settings.megamenu1_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
													<img class="autoHt lazyload"
													     src="{{ 'spacer.png' | asset_img_url: 'master' }}"
													     width="{{ block.settings.megamenu1_image.width }}"
													     height="{{ block.settings.megamenu1_image.height }}"
													     data-src="{{ img_url1 }}" data-widths="[180, 360, 540, 720]"
													     data-aspectratio="{{ block.settings.megamenu1_image.aspect_ratio }}"
													     data-sizes="auto"
													     alt="{{ block.settings.megamenu1_image.alt | escape }}">
													<span
														class="title">{{ block.settings.ttl1 | split: '[label' | first }} {% if block.settings.ttl1 contains '[label' %}
															<span
															class="lbl {{ m3label1 }}">{{ section.settings[m3label1] }}</span>{% endif %}</span>
												</a>
												<ul>
													{% for link in linklists[block.settings.mm_linkcol1].links %}
														<li class="lvl-2{% if link.active %} active{% endif %}">
															{%- assign label = link.title | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
															<a href="{{ link.url }}"
															   class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}
																	<span
																	class="lbl {{ label1 }}">{{ section.settings[label] }}</span>{% endif %}
															</a>
														</li>
													{% endfor %}
												</ul>
											</li>
										{%- endif -%}
										{%- if block.settings.ttl2 != blank -%}
											<li class="grid__item {{ mmcolumns }} lvl-1">
												{%- assign m3label2 = block.settings.ttl2 | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
												<a href="{% if block.settings.ttlurl2 != blank %}{{ block.settings.ttlurl2 }}{% else %}#{% endif %}"
												   class="lvl-1">
													{%- assign img_url2 = block.settings.megamenu2_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
													<img class="autoHt lazyload"
													     src="{{ 'spacer.png' | asset_img_url: 'master' }}"
													     width="{{ block.settings.megamenu2_image.width }}"
													     height="{{ block.settings.megamenu2_image.height }}"
													     data-src="{{ img_url2 }}" data-widths="[180, 360, 540, 720]"
													     data-aspectratio="{{ block.settings.megamenu2_image.aspect_ratio }}"
													     data-sizes="auto"
													     alt="{{ block.settings.megamenu2_image.alt | escape }}">
													<span
														class="title">{{ block.settings.ttl2 | split: '[label' | first }} {% if block.settings.ttl2 contains '[label' %}
															<span
															class="lbl {{ m3label2 }}">{{ section.settings[m3label2] }}</span>{% endif %}</span>
												</a>
												<ul>
													{% for link in linklists[block.settings.mm_linkcol2].links %}
														<li class="lvl-2{% if link.active %} active{% endif %}">
															{%- assign label = link.title | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
															<a href="{{ link.url }}"
															   class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}
																	<span
																	class="lbl {{ label1 }}">{{ section.settings[label] }}</span>{% endif %}
															</a>
														</li>
													{% endfor %}
												</ul>
											</li>
										{%- endif -%}
										{%- if block.settings.ttl3 != blank -%}
											<li class="grid__item {{ mmcolumns }} lvl-1">
												{%- assign m3label3 = block.settings.ttl3 | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
												<a href="{% if block.settings.ttlurl3 != blank %}{{ block.settings.ttlurl3 }}{% else %}#{% endif %}"
												   class="lvl-1">
													{%- assign img_url3 = block.settings.megamenu3_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
													<img class="autoHt lazyload"
													     src="{{ 'spacer.png' | asset_img_url: 'master' }}"
													     width="{{ block.settings.megamenu3_image.width }}"
													     height="{{ block.settings.megamenu3_image.height }}"
													     data-src="{{ img_url3 }}" data-widths="[180, 360, 540, 720]"
													     data-aspectratio="{{ block.settings.megamenu3_image.aspect_ratio }}"
													     data-sizes="auto"
													     alt="{{ block.settings.megamenu3_image.alt | escape }}">
													<span
														class="title">{{ block.settings.ttl3 | split: '[label' | first }} {% if block.settings.ttl3 contains '[label' %}
															<span
															class="lbl {{ m3label3 }}">{{ section.settings[m3label3] }}</span>{% endif %}</span>
												</a>
												<ul>
													{% for link in linklists[block.settings.mm_linkcol3].links %}
														<li class="lvl-2{% if link.active %} active{% endif %}">
															{%- assign label = link.title | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
															<a href="{{ link.url }}"
															   class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}
																	<span
																	class="lbl {{ label1 }}">{{ section.settings[label] }}</span>{% endif %}
															</a>
														</li>
													{% endfor %}
												</ul>
											</li>
										{%- endif -%}
										{%- if block.settings.ttl4 != blank -%}
											<li class="grid__item {{ mmcolumns }} lvl-1">
												{%- assign m3label4 = block.settings.ttl4 | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
												<a href="{% if block.settings.ttlurl4 != blank %}{{ block.settings.ttlurl4 }}{% else %}#{% endif %}"
												   class="lvl-1">
													{%- assign img_url4 = block.settings.megamenu4_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
													<img class="autoHt lazyload"
													     src="{{ 'spacer.png' | asset_img_url: 'master' }}"
													     width="{{ block.settings.megamenu4_image.width }}"
													     height="{{ block.settings.megamenu4_image.height }}"
													     data-src="{{ img_url4 }}" data-widths="[180, 360, 540, 720]"
													     data-aspectratio="{{ block.settings.megamenu4_image.aspect_ratio }}"
													     data-sizes="auto"
													     alt="{{ block.settings.megamenu4_image.alt | escape }}">
													<span
														class="title">{{ block.settings.ttl4 | split: '[label' | first }} {% if block.settings.ttl4 contains '[label' %}
															<span
															class="lbl {{ m3label4 }}">{{ section.settings[m3label4] }}</span>{% endif %}</span>
												</a>
												<ul>
													{% for link in linklists[block.settings.mm_linkcol4].links %}
														<li class="lvl-2{% if link.active %} active{% endif %}">
															{%- assign label = link.title | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
															<a href="{{ link.url }}"
															   class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}
																	<span
																	class="lbl {{ label1 }}">{{ section.settings[label] }}</span>{% endif %}
															</a>
														</li>
													{% endfor %}
												</ul>
											</li>
										{%- endif -%}
										{%- if block.settings.ttl5 != blank -%}
											<li class="grid__item {{ mmcolumns }} lvl-1">
												{%- assign m3label5 = block.settings.ttl5 | split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
												<a href="{% if block.settings.ttlurl5 != blank %}{{ block.settings.ttlurl5 }}{% else %}#{% endif %}"
												   class="lvl-1">
