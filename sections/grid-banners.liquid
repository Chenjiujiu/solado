{%- capture gridBanners -%}
<div class="content-indent {{ section.settings.top-offeset}}"> 
  {% capture imageGallery %}
  <div class="imgBnrOuter{% if section.settings.hide_mobile %} small--hide{% endif %} sec-{{section.id}}">
    
    <div class="{% if section.settings.fullwidth %}fullwidth-{{section.id}}{% else %}page-width{%endif%}">

      {%- if section.settings.title != blank  or section.settings.subtitle != blank -%}
      <div class="sec-ttl {{ settings.title_align}} fadeIn">
        <h3 class="h3">{{ section.settings.title }}</h3>
        {%- if section.settings.subtitle != blank -%}<span>{{section.settings.subtitle}}</span>{%- endif -%}                 
      </div>
      {% endif %}

      {% if section.settings.snow_effect %} 
      <div class="falling-snow{% if section.settings.snow_style == "style2" %} style2{% else %} style1{% endif %}">
      {% endif %}
        
     
      {%- for block in section.blocks -%}
      {%- if block.type == 'style1' -%}
      <div class="grid{% if section.settings.disable_zoom %} no-zoom{% endif %}" {{ block.shopify_attributes }}>
        <div class="grid__item large-up--one-half img-bnr wow fadeInUp" data-wow-delay="0ms">
          <div class="inner {{block.settings.bnr1align}}">{% include 'image-grid', image: block.settings.image1, title: block.settings.text1, subtitle: block.settings.text1-large, link: block.settings.link1, imgSize: '930x620' %}</div>
        </div>
        <div class="grid__item medium-up--one-half large-up--one-quarter img-bnr wow fadeInUp" data-wow-delay="50ms">
          <div class="inner {{block.settings.bnr2align}}">{% include 'image-grid', image: block.settings.image2, title: block.settings.text2, subtitle: block.settings.text2-large, link: block.settings.link2, imgSize: '460x300' %}</div>
        </div>
        <div class="grid__item medium-up--one-half large-up--one-quarter img-bnr wow fadeInUp" data-wow-delay="100ms">
          <div class="inner {{block.settings.bnr3align}}">{% include 'image-grid', image: block.settings.image3, title: block.settings.text3, subtitle: block.settings.text3-large, link: block.settings.link3, imgSize: '460x300' %}</div>
        </div>
        <div class="grid__item large-up--one-half img-bnr wow fadeInUp" data-wow-delay="150ms">
          <div class="inner {{block.settings.bnr4align}}">{% include 'image-grid', image: block.settings.image4, title: block.settings.text4, subtitle: block.settings.text4-large, link: block.settings.link4, imgSize: '930x300' %}</div>
        </div>
      </div>
      {%- endif -%}

      {%- if block.type == 'style2' -%}
      <div class="grid{% if section.settings.disable_zoom %} no-zoom{% endif %}" {{ block.shopify_attributes }}>
        <div class="grid__item medium-up--one-quarter">
          <div class="img-bnr wow fadeInUp" data-wow-delay="0ms">
            <div class="inner {{block.settings.bnr1align}}">{% include 'image-grid', image: block.settings.image1, title: block.settings.text1, subtitle: block.settings.text1-large, link: block.settings.link1, imgSize: '460x460' %}</div>
          </div>
          <div class="img-bnr wow fadeInUp" data-wow-delay="50ms">
            <div class="inner {{block.settings.bnr2align}}">{% include 'image-grid', image: block.settings.image2, title: block.settings.text2, subtitle: block.settings.text2-large, link: block.settings.link2, imgSize: '460x460' %}</div>
          </div>
        </div>
        <div class="grid__item medium-up--one-half img-bnr wow fadeInUp" data-wow-delay="100ms">
          <div class="inner {{block.settings.bnr3align}}">{% include 'image-grid', image: block.settings.image3, title: block.settings.text3, subtitle: block.settings.text3-large, link: block.settings.link3, imgSize: '930x930' %}</div>
        </div>
        <div class="grid__item medium-up--one-quarter">
          <div class="img-bnr wow fadeInUp" data-wow-delay="150ms">
            <div class="inner {{block.settings.bnr4align}}">{% include 'image-grid', image: block.settings.image4, title: block.settings.text4, subtitle: block.settings.text4-large, link: block.settings.link4, imgSize: '460x460' %}</div>
          </div>
          <div class="img-bnr wow fadeInUp" data-wow-delay="200ms">
            <div class="inner {{block.settings.bnr5align}}">{% include 'image-grid', image: block.settings.image5, title: block.settings.text5, subtitle: block.settings.text5-large, link: block.settings.link5, imgSize: '460x460' %}</div>
          </div>
        </div>
      </div>
      {%- endif -%}

      {%- if block.type == 'style3' -%}
      <div class="grid{% if section.settings.disable_zoom %} no-zoom{% endif %}" {{ block.shopify_attributes }}>
        <div class="grid__item medium-up--one-half img-bnr wow fadeInUp" data-wow-delay="0ms">
          <div class="inner {{block.settings.bnr1align}}">{% include 'image-grid', image: block.settings.image1, title: block.settings.text1, subtitle: block.settings.text1-large, link: block.settings.link1, imgSize: '930x930' %}</div>
        </div>
        <div class="grid__item medium-up--one-half img-bnr wow fadeInUp" data-wow-delay="50ms">
          <div class="inner mb20 {{block.settings.bnr2align}}">{% include 'image-grid', image: block.settings.image2, title: block.settings.text2, subtitle: block.settings.text2-large, link: block.settings.link2, imgSize: '930x455' %}</div>
          <div class="inner mb20 {{block.settings.bnr3align}}">{% include 'image-grid', image: block.settings.image3, title: block.settings.text3, subtitle: block.settings.text3-large, link: block.settings.link3, imgSize: '930x455' %}</div>             
        </div>                    
      </div>
      {%- endif -%}

      {%- if block.type == 'style4' -%}
      <div class="grid{% if section.settings.disable_zoom %} no-zoom{% endif %}" {{ block.shopify_attributes }}>
        <div class="grid__item one-whole medium-up--one-whole large-up--one-half img-bnr wow fadeInUp" data-wow-delay="0ms">
          <div class="inner {{block.settings.bnr1align}}">{% include 'image-grid', image: block.settings.image1, title: block.settings.text1, subtitle: block.settings.text1-large, link: block.settings.link1, imgSize: '930x700' %}</div>
        </div>
        <div class="grid__item one-half medium-up--one-half large-up--one-quarter img-bnr wow fadeInUp" data-wow-delay="50ms">
          <div class="inner {{block.settings.bnr2align}}">{% include 'image-grid', image: block.settings.image2, title: block.settings.text2, subtitle: block.settings.text2-large, link: block.settings.link2, imgSize: '460x718' %}</div>
        </div>
        <div class="grid__item one-half medium-up--one-half large-up--one-quarter img-bnr wow fadeInUp" data-wow-delay="100ms">
          <div class="inner mb20 {{block.settings.bnr3align}}">{% include 'image-grid', image: block.settings.image3, title: block.settings.text3, subtitle: block.settings.text3-large, link: block.settings.link3, imgSize: '460x342' %}</div>
          <div class="inner mb20 {{block.settings.bnr4align}}">{% include 'image-grid', image: block.settings.image4, title: block.settings.text4, subtitle: block.settings.text4-large, link: block.settings.link4, imgSize: '460x342' %}</div>                    
        </div>
      </div>
      {%- endif -%}

      {%- if block.type == 'style5' -%}
      <div class="grid{% if section.settings.disable_zoom %} no-zoom{% endif %}" {{ block.shopify_attributes }}>
        <div class="grid__item large-up--two-fifths">
          <div class="grid__item img-bnr medium-up--one-half large-up--one-whole wow fadeInUp" data-wow-delay="0ms">
            <div class="inner {{block.settings.bnr1align}}">{% include 'image-grid', image: block.settings.image1, title: block.settings.text1, subtitle: block.settings.text1-large, link: block.settings.link1, imgSize: '712x340' %}</div>
          </div>
          <div class="grid__item img-bnr medium-up--one-half large-up--one-whole wow fadeInUp" data-wow-delay="50ms">
            <div class="inner {{block.settings.bnr2align}}">{% include 'image-grid', image: block.settings.image2, title: block.settings.text2, subtitle: block.settings.text2-large, link: block.settings.link2, imgSize: '712x340' %}</div>
