  {{imageGallery | strip_newlines | remove: "  " | remove: "	" }}
</div>


<style>
  .imgBnrOuter.sec-{{section.id}} .inner .ttl { z-index:1; {% if section.settings.bnr_ttl_bg != blank %} background-color:rgba({{ section.settings.bnr_ttl_bg}},0.8); {% endif %}
    color:{{ section.settings.bnr_ttl }};
  }
  .imgBnrOuter.sec-{{section.id}} .inner .ttl:before {
    position:absolute; left:0; top:0; content:""; width:100%; height:100%; background:{{section.settings.bnr_ttl_bg}};
    opacity:{{ section.settings.bnr_ttl_bg_op}}; z-index:-1;
  }
  {% if section.settings.fullwidth %}
    .fullwidth-{{section.id}} { padding-left:{{ section.settings.pad-desk}}px; padding-right:{{ section.settings.pad-desk}}px; }
  	@media only screen and (max-width: 1024px) {
     .fullwidth-{{section.id}} { padding-left:{{ section.settings.pad-ipad}}px; padding-right:{{ section.settings.pad-ipad}}px; }
   	} 
  	@media only screen and (max-width: 767px) {
     .fullwidth-{{section.id}} { padding-left:{{ section.settings.pad-mbl}}px; padding-right:{{ section.settings.pad-mbl}}px; }
   	} 
  {% endif %}
</style>
{%- endcapture -%}
{{gridBanners | strip_newlines | remove: "  " | remove: "	" }}

{% schema %}
