        </div>
      </div>
      {%- endif -%}

      {%- if block.type == 'style6' -%}
      <div class="grid style6{% if section.settings.disable_zoom %} no-zoom{% endif %}" {{ block.shopify_attributes }}>
        <div class="grid__item medium-up--one-third img-bnr wow fadeInUp" data-wow-delay="0ms">                        
          <div class="inner {{block.settings.bnr1align}}">{% include 'image-grid', image: block.settings.image1, title: block.settings.text1, subtitle: block.settings.text1-large, link: block.settings.link1, imgSize: '640x640' %}</div>

        </div>
        <div class="grid__item medium-up--one-third img-bnr wow fadeInUp" data-wow-delay="50ms">
          <div class="inner {{block.settings.bnr2align}}">{% include 'image-grid', image: block.settings.image2, title: block.settings.text2, subtitle: block.settings.text2-large, link: block.settings.link2, imgSize: '640x640' %}</div>
