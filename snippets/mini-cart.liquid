{{ 'cart-shipping-fortunately.css' | asset_url | stylesheet_tag }}{{ 'cart-shipping-fortunately.js' | asset_url | script_tag }}<div id="cart-drawer" class="block block-cart">	<a href="javascript:void(0);" class="close-cart"><i class="ad ad-times-l" aria-hidden="true"></i></a>	<h4>{{ 'cart.general.title' | t }}</h4>	<ul class="mini-products-list" data-cart-view="data-cart-view">		<li id="cart-title" rv-show="cart.item_count | lt 1">{{ 'cart.general.empty' | t }}</li>		<li class="grid" rv-each-item="cart.items">			<a class="grid__item one-third medium-up--one-quarter product-image" rv-href="item.url"><img class="imgFl" rv-src="item.image | productImageSize 'medium'" width="98" height="148" rv-alt="item.title" /></a>			<div class="grid__item two-thirds medium-up--three-quarters product-details">              	<a href="#" class="remove" rv-data-cart-remove="index | plus 1"><i class="ad ad-times-l" aria-hidden="true"></i></a>              	<a class="pName" rv-href="item.url" rv-text="item.product_title"></a>				<!--  <div class="vropts" rv-text="item.variant_title"></div> -->              	<div class="vropts" rv-text="item.selling_plan_allocation.selling_plan.name" rv-show="item.selling_plan_allocation | lt 1"></div>				<ul rv-hide="item.propertyArray | empty">					<li rv-each-property="item.propertyArray < properties"><small class="text-muted">{property.name}: {property.value}</small></li>				</ul>              	<div class="priceRow flex">				  {% comment %}<span>{{item}} - item= {{item | json}} - {{item.price | money}}</span>{% endcomment %}                  <span class="product-price" rv-html="item.price | money"></span>              	</div>              	<ul class="solado-minicart-options">                  <li rv-each-option="item.options_with_values">                    <div class="solado-minicart-options__color" rv-if="option.name | isColorOprion" rv-style="option.value | backgroundColor"></div>                    <div class="solado-minicart-options-size" rv-unless="option.name | isColorOprion" rv-text="option.value"></div>                  </li>              	</ul>              	<div class="qtyField" title="{{ 'products.product.quantity' | t }}">                    <a class="qtyBtn" href="#" rv-data-cart-update="index | plus 1" rv-data-cart-quantity="item.quantity | minus 1" rv-if="item.quantity | noteq 1"><i class="ad ad-minus-r"></i></a>                    <a class="qtyBtn" href="#" rv-data-cart-update="index | plus 1" rv-data-cart-quantity="item.quantity | minus 1" rv-if="item.quantity | eq 1"><i><svg t="1661235323013" class="icon" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="3208" width="32" height="32"><path d="M595.456 745.984c-17.408 0-31.232-13.824-31.232-31.232V441.344c0-17.408 13.824-31.232 31.232-31.232s31.232 13.824 31.232 31.232v272.896c0 17.408-14.336 31.744-31.232 31.744zM428.544 745.984c-17.408 0-31.232-13.824-31.232-31.232V441.344c0-17.408 13.824-31.232 31.232-31.232s31.232 13.824 31.232 31.232v272.896c0 17.408-13.824 31.744-31.232 31.744zM875.008 252.416H148.992c-17.408 0-31.232-13.824-31.232-31.232 0-17.408 13.824-31.232 31.232-31.232h726.016c17.408 0 31.232 13.824 31.232 31.232 0 17.408-13.824 31.232-31.232 31.232z" fill="#040000" p-id="3209"></path><path d="M731.648 966.144H291.84c-46.08 0-81.92-46.08-81.92-104.448V189.952h604.16v671.232c-1.024 58.88-36.864 104.96-82.432 104.96zM272.384 252.416v608.768c0 25.6 12.8 42.496 19.456 42.496h440.32c6.656 0 19.456-16.896 19.968-42.496V252.416H272.384z" fill="#040000" p-id="3210"></path><path d="M636.928 245.248c-17.408 0-31.232-13.824-31.232-31.232 0-51.712-41.984-93.696-93.696-93.696s-93.696 41.984-93.696 93.696c0 17.408-13.824 31.232-31.232 31.232s-31.232-13.824-31.232-31.232c0-86.016 70.144-156.16 156.16-156.16s156.16 70.144 156.16 156.16c0 17.408-13.824 31.232-31.232 31.232z" fill="#040000" p-id="3211"></path></svg></i></a>                    <span class="qtyBtn" rv-text="item.quantity"></span>                    <a class="qtyBtn" href="#" rv-data-cart-update="index | plus 1" rv-data-cart-quantity="item.quantity | plus 1"><i class="ad ad-plus-r"></i></a>                </div>			</div>		</li>	</ul>  	<ul class="minicartTol" data-cart-view="data-cart-view" rv-show="cart.item_count | gt 0">        {%- if settings.free_ship_minicart and settings.free_ship != blank and settings.free_ship_msg != blank -%}        {%- assign shipProd = all_products[settings.free_ship] -%}        <li id="freeShipMsg" class="freeShipMsg" data-price="{{shipProd.price}}"><i class="ad ad-truck-l"></i><p>{{settings.free_ship_msg | replace: '$','<b class="freeShip"></b>' }}</p></li>        <li id="freeShipclaim" class="freeShipMsg freeShipclaim"><i class="ad ad-truck-l"></i><p>{{settings.got_free_ship }}</p></li>        {%- endif -%}				{% comment %}<span class="ajax-cart__header__continue-link-text">{{ 'cart.general.continue_shopping' | t }}</span> {% endcomment %}        <li>{% render 'cart-shipping-fortunately', cart %}</li>		<li class="cart-subtotal-row flex"> <span>{{ 'cart.general.subtotal' | t }}:</span> <span class="product-price" rv-html="cart.total_price | money currency.currentCurrency"></span> </li>      	<li class="buttonSet">          	{%- if settings.shippingTax -%}<p class="cart__shipping">{{ 'cart.general.shipping_at_checkout' | t }}</p>{%- endif -%}          	<div class="mini-cart-btn-wrap">          	{% unless settings.hide_checkout_cart == true %}			{%- if settings.terms_checkbox -%}				<div class="cart_tearm">					<label for="cartDWtearm"><input type="checkbox" name="tearm" id="cartDWtearm" class="checkbox custCheck" value="tearm" required /><span class="checkbox"></span> {{ settings.terms_text}}</label>				</div>			{%- endif -%}			<form action="{{ routes.cart_url }}" method="post" novalidate>				<input type="submit" name="checkout" id="cartDWcheckout" class="btn cartDWcheckout {{settings.btn-style}}" value="{{ 'cart.general.checkout' | t }}" {% if settings.terms_checkbox %}disabled="disabled"{% endif %} />			</form>            {% endunless %}          	<a id="inlinecheckout-cart" href="{{ routes.cart_url }}" class="btn {{settings.btn-style2}}">{{ 'cart.general.view_cart' | t }}</a>          	</div>            <div class="mini-cart-discount"></div>          	<div style="text-align:center;">Coupons can be applied during checkout.</div>		</li>	</ul></div><style>  .mini-products-list .one-third{  	width:98px;  }  .mini-products-list .two-thirds{  	width:calc(100% - 98px);  }  .mini-products-list li ul.solado-minicart-options {  	display: flex;    align-items: center;    margin:0 0 18px 0  }  .solado-minicart-options__color {  	overflow: hidden;    display: inline-block;    position: relative;    height: 20px;    width: 20px;    margin-right: 11px;    cursor: pointer;    border-radius: 50%;    box-shadow: 0 0 1px 1px #dddddd;    border: 1px solid #ffffff  }  .solado-minicart-options-size {    text-align: center;    border-radius: 2px;    font-style: normal;    font-weight: 600;    font-size: 14px;    line-height: 20px;  }  #cart-drawer .mini-cart-discount-img{  	margin-top:15px;    margin-bottom:12px;  }  #cart-drawer .mini-products-list .imgFl{  	width:98px;height:148px;  }  #cart-drawer .mini-cart-btn-wrap{  	display: flex;  }  #cart-drawer .mini-cart-btn-wrap form {  	width: 50%;    margin-right: 4px;  }</style>{% comment %}{% render 'currency-transfer' %}{% endcomment %}<script type="text/javascript">  window.addEventListener('DOMContentLoaded',function(){    /* 处理折扣码 */    handleCodeText('#cart-drawer .mini-cart-discount');    /** 解决滑动的时候触发了a标签，导致iphone卡得划不动的问题 **/    $("#cart-drawer").on("touchmove",".product-image",function(e){    let x = e.target.x;      let y = e.target.y;      let w = x<0?x*-1:x;      let h = y<0?y*-1:y;      if(w>h){         event.preventDefault();      }    });    /** Taboola Pixels checkout code for mini cart **/    $("#cartDWcheckout").click(function(e){        _tfa.push({notify: 'event', name: 'start_checkout', id: 1461029});    });    $("#cart-drawer .cart-shipping-fortunately").on('click',function(){        location.href ="/collections/back-in-stock";    });    /** this code for mini cart to refresh top shopping info **/    $(document).on("cart.requestComplete", function (event, cart) {        /*console.log('event', event);        console.log('change mini cart',cart);*/        let $cartDrawr = $("#cart-drawer");        let $shoppingFor = $cartDrawr.find('.cart-shipping-fortunately');        if(cart.item_count >= 0){            $shoppingFor.show();            $.ajax({                type: 'GET',                dataType: 'html',                url: '/cart?'+new Date().getTime(),                success(data) {                    $("body").addClass("loading");                    $('form.cart div:first').html($("form.cart div:first", data).html());                    $('form.cart .cart-pay-wrap').html($("form.cart .cart-pay-wrap", data).html());                    $('.cartSummery').html($(".cartSummery", data).html());                    let $cartDrawr = $("#cart-drawer");                    let newDomForm = $('#cart-template-form' ,data);                    let pageDomForm =  $('#cart-template-form');                    let newDomFortunately = $('.cart-shipping-fortunately:not(.inCartPage)' ,data);                    let pageDomFortunately =  $cartDrawr.find(".cart-shipping-fortunately").parent();                    if(pageDomForm.length > 0){                        pageDomForm.html(newDomForm);                        handleCodeText('#cart-template-form .cart-template-discount-code');                    }                    pageDomFortunately.html(newDomFortunately);                    initCartProgress('body');                    $("#cart-drawer .cart-shipping-fortunately").on('click',function(){                        location.href ="/collections/back-in-stock";                    });                    $('body').removeClass('loading hideOverly');                }            });        }else{            $shoppingFor.hide();        }    });    /** 查询是否为color **/    rivets.formatters.isColorOprion = function(value){        return value === 'color' || value === 'Color';    };    /** 查询是否为size **/    rivets.formatters.isSizeOprion = function(value){        return value === 'size';    };    /** 初始化style **/    rivets.formatters.backgroundColor = function(value){        return 'background-color:' +  value    };    rivets.formatters.eq = function(value, checkAgainst) {        return (value == checkAgainst);    };    rivets.formatters.noteq = function(value, checkAgainst) {        return (value != checkAgainst);    }; });</script>