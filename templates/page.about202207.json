    "1659702522cc9fb9cb": {
      "type": "news-list-202208",
      "blocks": {
        "1659702522215ea206-0": {
          "type": "html",
          "settings": {
            "code": "<div class=\"one-news\">\n                <div class=\"header-line\"><\/div>\n                <li class=\"news-title\">AUGUST 2021<\/li>\n                <div class=\"news-content\">With the ambition to influence the future of fast fashion and do things a little differently, Solado was born.<\/div>\n            <\/div>",
            "width": "33.3333%"
          }
        },
        "1659702522215ea206-1": {
          "type": "html",
          "settings": {
            "code": "<div class=\"one-news\">\n                <div class=\"header-line\"><\/div>\n                <li class=\"news-title\">SEPTEMBER 2021<\/li>\n                <div class=\"news-content\">We officially welcomed our U.K team to the Solado family.<\/div>\n            <\/div>",
            "width": "33.3333%"
          }
        },
        "f07bbd57-54c8-4829-ab69-14a831d856d2": {
          "type": "html",
          "settings": {
            "code": "<div class=\"one-news\">\n                <div class=\"header-line\"><\/div>\n                <li class=\"news-title\">NOVEMBER 2021<\/li>\n                <div class=\"news-content\">Our first ever influencer collaboration and brand campaign \"Wear WTF You Want\" went live.<\/div>\n            <\/div>",
            "width": "33.3333%"
          }
        },
        "d0350159-3c5d-46cc-95ca-f149ee092b57": {
          "type": "html",
          "settings": {
            "code": "<div class=\"one-news\">\n                <div class=\"header-line\"><\/div>\n                <li class=\"news-title\">JANUARY 2022<\/li>\n                <div class=\"news-content\">We launched our biodegradable packaging for all of our influencer parcel send outs.\n                    We Partnered with TRACE and gained carbon neutral status by supporting certified carbon credits and planting more than 2,270 trees all around the world. Click here to see how we are taking climate action <a href=\"https:\/\/www.our-trace.com\/brand\/solado\">https:\/\/www.our-trace.com\/brand\/solado<\/a>\n<\/div>\n            <\/div>",
            "width": "50%"
          }
        },
        "d46ede44-9f96-4b5e-8576-d801909f79c7": {
          "type": "html",
          "settings": {
            "code": "<div class=\"one-news\">\n                <div class=\"header-line\"><\/div>\n                <li class=\"news-title\">FEBRUARY 2022<\/li>\n                <div class=\"news-content\">We launched our second brand campaign, \"We're Taking Sexy Back,\" with the aim to inspire our customer to reclaim what looking and feeling sexy feels like on your own terms.<\/div>\n            <\/div>",
            "width": "50%"
          }
        }
      },
